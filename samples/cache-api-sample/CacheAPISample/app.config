<?xml version="1.0" encoding="utf-8"?>
<configuration>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup>
  <appSettings>
  <!-- This is the name of the cache -->
    <add key="CacheId" value="mycache"/>
	<!-- Default flag whether items added in cache should be expirable or permenant -->
    <add key="Expirable" value="False"/>
	<!-- Expiration time in Hour, Minutes, Seconds format for TimeSpan -->
    <add key="TTL" value="6:12:14"/>
  </appSettings>
</configuration>
